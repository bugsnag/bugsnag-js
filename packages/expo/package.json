{
  "name": "@bugsnag/expo",
  "version": "0.0.0",
  "main": "src/notifier.js",
  "types": "dist/types/bugsnag.d.ts",
  "description": "Bugsnag error reporter for Expo applications",
  "keywords": [
    "bugsnag",
    "expo",
    "react native",
    "exception",
    "error",
    "javascript",
    "monitoring",
    "reporting",
    "stability",
    "mobile"
  ],
  "homepage": "https://www.bugsnag.com/",
  "repository": {
    "type": "git",
    "url": "git@github.com:bugsnag/bugsnag-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src/notifier.js",
    "src/config.js",
    "hooks/post-publish.js",
    "dist"
  ],
  "scripts": {
