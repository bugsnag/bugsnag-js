    "bundle-types": "../../bin/bundle-types",
    "build": "npm run clean && npm run bundle-types",
    "test:types": "tslint -c ../../tslint.json types/*.d.ts && jasmine 'types/**/*.test.js'",
    "postversion": "npm run build"
  },
  "author": "Bugsnag",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "jasmine": "^3.1.0",
    "nyc": "^12.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "@bugsnag/core": "^6.0.0",
    "@bugsnag/plugin-expo-app": "^6.0.0",
    "@bugsnag/plugin-react-native-global-error-handler": "^6.0.0",
    "@bugsnag/plugin-react-native-unhandled-rejection": "^6.0.0",
    "bugsnag-build-reporter": "^1.0.1",
    "bugsnag-sourcemaps": "^1.1.0"
