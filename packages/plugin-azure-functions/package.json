  "description": "Azure Functions support for @bugsnag/node",
  "homepage": "https://www.bugsnag.com/",
  "repository": {
    "type": "git",
    "url": "git@github.com:bugsnag/bugsnag-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "types"
