  "dependencies": {
    "@bugsnag/in-flight": "^7.11.0",
    "@bugsnag/plugin-browser-session": "^7.11.0"
  },
  "devDependencies": {
    "@bugsnag/core": "^7.11.0"
  },
  "peerDependencies": {
    "@bugsnag/core": "^7.0.0"
  }
}
