{
  "name": "@bugsnag/react-native",
  "version": "7.0.0-rc.4",
  "main": "src/notifier.js",
  "types": "dist/types/bugsnag.d.ts",
  "description": "Bugsnag error reporter for React Native applications",
  "keywords": [
    "bugsnag",
    "react native",
    "exception",
    "error",
    "javascript",
    "monitoring",
    "reporting",
    "stability",
    "mobile"
  ],
  "homepage": "https://www.bugsnag.com/",
  "repository": {
    "type": "git",
    "url": "git@github.com:bugsnag/bugsnag-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "rnpm": {
    "ios": {
      "project": "ios/BugsnagReactNative.xcodeproj",
      "sharedLibraries": [
        "libz"
      ]
    },
    "android": {
      "packageInstance": "new BugsnagPackage()",
      "packageImportPath": "import com.bugsnag.android.BugsnagPackage;"
    }
  },
  "files": [
    "src",
    "dist",
    "android/build.gradle",
    "android/proguard-rules.pro",
    "android/src/main",
    "ios/Bugsnag",
    "ios/BugsnagReactNative",
    "ios/BugsnagReactNative.xcodeproj",
    "ios/vendor/bugsnag-cocoa/Configurations/Config.xcconfig",
    "ios/vendor/bugsnag-cocoa/Source/**/*.{h,m,mm,cpp,c}",
    "ios/vendor/bugsnag-cocoa/iOS/{Bugsnag.xcodeproj,Info.plist,module.modulemap}",
    "BugsnagReactNative.podspec",
    "react-native.config.js"
  ],
  "scripts": {
    "clean": "rm -fr dist && mkdir dist",
    "bundle-types": "../../bin/bundle-types bugsnag",
    "build": "npm run clean && npm run bundle-types",
    "test:types": "jasmine 'types/**/*.test.js'",
    "postversion": "npm run build"
  },
  "author": "Bugsnag",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "jasmine": "^3.5.0",
    "nyc": "^12.0.2",
    "react-native": "^0.59.9",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "@bugsnag/core": "7.0.0-rc.4",
    "@bugsnag/delivery-react-native": "7.0.0-rc.4",
    "@bugsnag/plugin-console-breadcrumbs": "7.0.0-rc.4",
    "@bugsnag/plugin-network-breadcrumbs": "7.0.0-rc.4",
    "@bugsnag/plugin-react": "7.0.0-rc.4",
    "@bugsnag/plugin-react-native-client-sync": "7.0.0-rc.4",
    "@bugsnag/plugin-react-native-event-sync": "7.0.0-rc.4",
    "@bugsnag/plugin-react-native-global-error-handler": "7.0.0-rc.4",
    "@bugsnag/plugin-react-native-session": "7.0.0-rc.4",
    "@bugsnag/plugin-react-native-unhandled-rejection": "7.0.0-rc.4",
    "iserror": "^0.0.2"
  }
}
