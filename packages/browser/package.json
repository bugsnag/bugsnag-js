{
  "name": "@bugsnag/browser",
  "version": "5.0.0-alpha.0",
  "main": "dist/bugsnag.js",
  "types": "types/bugsnag.d.ts",
  "description": "Bugsnag error reporter for browser JavaScript",
  "homepage": "https://www.bugsnag.com/",
  "repository": {
    "type": "git",
    "url": "git@github.com:bugsnag/bugsnag-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "size": "./bin/size",
    "clean": "rm -fr dist && mkdir dist",
    "build": "npm run clean && npm run build:dist && npm run build:dist:min",
    "build:dist": "NODE_ENV=production IS_BROWSER=yes bin/bundle | exorcist dist/bugsnag.js.map > dist/bugsnag.js",
    "build:dist:min": "NODE_ENV=production IS_BROWSER=yes bin/bundle | bin/minify dist/bugsnag.min.js",
    "test:types": "tslint -c ../../tslint.json types/*.d.ts && jasmine 'types/**/*.test.js'",
    "test": "bundle exec maze-runner"
  },
  "author": "Bugsnag",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@bugsnag/core": "^5.0.0-alpha.0",
    "@bugsnag/delivery-x-domain-request": "^5.0.0-alpha.0",
    "@bugsnag/delivery-xml-http-request": "^5.0.0-alpha.0",
    "@bugsnag/plugin-browser-context": "^5.0.0-alpha.0",
    "@bugsnag/plugin-browser-device": "^5.0.0-alpha.0",
    "@bugsnag/plugin-browser-request": "^5.0.0-alpha.0",
    "@bugsnag/plugin-browser-session": "^5.0.0-alpha.0",
    "@bugsnag/plugin-client-ip": "^5.0.0-alpha.0",
    "@bugsnag/plugin-console-breadcrumbs": "^5.0.0-alpha.0",
    "@bugsnag/plugin-inline-script-content": "^5.0.0-alpha.0",
    "@bugsnag/plugin-interaction-breadcrumbs": "^5.0.0-alpha.0",
    "@bugsnag/plugin-navigation-breadcrumbs": "^5.0.0-alpha.0",
    "@bugsnag/plugin-network-breadcrumbs": "^5.0.0-alpha.0",
    "@bugsnag/plugin-simple-throttle": "^5.0.0-alpha.0",
    "@bugsnag/plugin-strip-query-string": "^5.0.0-alpha.0",
    "@bugsnag/plugin-window-onerror": "^5.0.0-alpha.0",
    "@bugsnag/plugin-window-unhandled-rejection": "^5.0.0-alpha.0",
    "exorcist": "^1.0.1",
    "jasmine": "^3.1.0",
    "nyc": "^12.0.2",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2",
    "uglify-js": "^3.4.3"
  }
}
