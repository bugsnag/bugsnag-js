    "command-line-usage": "^6.1.0",
    "consola": "^2.15.0",
    "detect-indent": "^6.1.0",
    "glob": "^7.1.6",
    "plist": "^3.0.1",
    "prompts": "^2.4.0",
    "semver": "^7.5.4",
    "xcode": "^3.0.1"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/command-line-usage": "^5.0.1",
    "@types/glob": "^8.1.0",
    "@types/prompts": "^2.0.9",
    "@types/semver": "^7.5.0",
    "typescript": "^4.1.3"
  }
}
