{
  "name": "@bugsnag/react-native-cli",
  "version": "8.1.0",
  "description": "A tool to help integrate Bugsnag with a React Native app",
  "bin": {
    "bugsnag-react-native-cli": "bin/cli"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
